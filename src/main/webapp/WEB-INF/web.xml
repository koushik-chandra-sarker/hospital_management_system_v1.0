<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
        version="4.0">
  <display-name>Archetype Created Web Application</display-name>
  <welcome-file-list>
    <welcome-file>login.jsp</welcome-file>
  </welcome-file-list>
  
  
  <servlet>
    <servlet-name>getAllUsernameServlet</servlet-name>
    <servlet-class>Servlet.getAllUsername</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>getAllUsernameServlet</servlet-name>
    <url-pattern>/getAllUsernameServlet</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>addDoctorServlet</servlet-name>
    <servlet-class>Servlet.addDoctorServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>addDoctorServlet</servlet-name>
    <url-pattern>/addDoctorServlet</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>updateDoctorServlet</servlet-name>
    <servlet-class>Servlet.updateDoctorServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>updateDoctorServlet</servlet-name>
    <url-pattern>/updateDoctorServlet</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>DeleteDoctorServlet</servlet-name>
    <servlet-class>Servlet.DeleteDoctorServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DeleteDoctorServlet</servlet-name>
    <url-pattern>/DeleteDoctorServlet</url-pattern>
  </servlet-mapping>


</web-app>
